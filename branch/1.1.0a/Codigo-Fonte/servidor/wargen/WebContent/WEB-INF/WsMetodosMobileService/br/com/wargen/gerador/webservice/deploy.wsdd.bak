<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from WsMetodosMobileService WSDL service -->

  <service name="WsMetodosMobile" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://webservice.gerador.wargen.com.br"/>
      <parameter name="wsdlServiceElement" value="WsMetodosMobileService"/>
      <parameter name="schemaQualified" value="http://webservice.gerador.wargen.com.br,http://bean.gerador.wargen.com.br"/>
      <parameter name="wsdlServicePort" value="WsMetodosMobile"/>
      <parameter name="className" value="br.com.wargen.gerador.webservice.WsMetodosMobileSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="WsMetodosMobile"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="fazerLogin" qname="operNS:fazerLogin" xmlns:operNS="http://webservice.gerador.wargen.com.br" returnQName="retNS:fazerLoginReturn" xmlns:retNS="http://webservice.gerador.wargen.com.br" returnType="rtns:UsuarioBean" xmlns:rtns="http://bean.gerador.wargen.com.br" soapAction="" >
        <parameter qname="pns:login" xmlns:pns="http://webservice.gerador.wargen.com.br" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:senha" xmlns:pns="http://webservice.gerador.wargen.com.br" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="testarConexao" qname="operNS:testarConexao" xmlns:operNS="http://webservice.gerador.wargen.com.br" returnQName="retNS:testarConexaoReturn" xmlns:retNS="http://webservice.gerador.wargen.com.br" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="fazerLogin testarConexao"/>

      <typeMapping
        xmlns:ns="http://bean.gerador.wargen.com.br"
        qname="ns:UsuarioBean"
        type="java:br.com.wargen.gerador.bean.UsuarioBean"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
